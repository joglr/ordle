{
  "name": "ordle",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@mui/material": "^5.4.0",
    "@mui/styled-engine-sc": "^5.3.0",
    "@types/next": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "clsx": "^1.1.1",
    "jest": "^27.4.7",
    "next": "12.0.9",
    "react": "17.0.2",
    "react-confetti": "^6.0.1",
    "react-dom": "17.0.2",
    "react-toast-notifications": "^2.5.1",
    "react-use": "^17.3.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.14",
    "@types/react": "17.0.38",
    "@vercel/node": "^1.12.1",
    "babel-jest": "^27.4.6",
    "eslint": "^8.9.0",
    "eslint-config-next": "12.0.7",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^12.1.7",
    "prettier": "2.5.1",
    "tsc-files": "^1.1.3",
    "typescript": "4.5.4"
  },
  "resolutions": {
    "@mui/styled-engine": "npm:@mui/styled-engine-sc@latest"
  },
  "resolve": {
    "alias": {
      "@mui/styled-engine": "@mui/styled-engine-sc"
    }
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "moduleNameMapper": {
      "\\.(scss|sass|css)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": [
        "babel-jest",
        {
          "presets": [
            "next/babel"
          ]
        }
      ]
    }
  },
  "prettier": {},
  "lint-staged": {
    "**/*.ts": [
      "eslint --cache --fix",
      "tsc-files --noEmit",
      "jest --bail --findRelatedTests"
    ],
    "**/*.{ts,js,css,md}": "prettier --write"
  }
}
